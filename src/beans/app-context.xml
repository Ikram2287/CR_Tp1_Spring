<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

   <!-- <bean id="gestionMaterielService" class="com.service.impl.GestionMaterielServiceImpl"/>-->

    <bean id="gestionMaterielController" class="com.controller.GestionMaterielController">
        <property name="gestionMaterielService" ref="gestionMaterielService"/>
    </bean>

    <bean id="gestionMaterielService" class="com.service.impl.GestionMaterielServiceImpl">
        <property name="materialDao" ref="materielDao" />
        <property name="listMateriel" ref ="listMateriel"/>
        <property name="nbChaise" value="2"/>
        <property name="nbBureau" value="2"/>
    </bean>

    <bean id="materielDao" class="com.database.impl.MaterialDaoImpl" ><!--init-method="init"-->
        <constructor-arg ref="listMateriel"/>
    </bean>
    <bean id="listMateriel" class="com.database.impl.listMaterielImpl">

    </bean>


</beans>